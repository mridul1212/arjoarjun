#include <iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    for(int i=0;i<n;i++)
	{
		int a,sum=0;
		cin>>a;
		cout<<"Case "<<i+1<<":"<<endl;
		for(int i=0;i<a;i++)
		{
			string str;
			int k;
			cin>>str;
			if(str=="donate")
			{
				cin>>k;
				sum+=k;
			}
		else
			{
			cout<<sum<<endl;
			}
		}
	}
    return 0;
}
